#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n],flag=0;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int key;
    cin>>key;
    for(int i=0;i<n;i++){
    if(key==arr[i])
    {
        flag=1;
    }
    }
    if(flag==1)
    {
        cout<<"True";
    }
    else
    {
        cout<<"False";
    }
    return 0;
}
