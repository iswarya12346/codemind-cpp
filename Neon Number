#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int rem,sum=0,sqr;
     sqr=n*n;
    while(sqr!=0)
    {
        rem=sqr%10;
        sum=sum+rem;
        sqr=sqr/10;
    }
     if(sum==n)
     {
         cout<<"Neon Number";
     }
     else
     {
         cout<<"Not Neon Number";
     }
     return 0;
}
